{
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": {
    "mode": "system",
    "dark": "Tokyo Night",
    "light": "Tokyo Night Light"
  },
  "inlay_hints": {
    "enabled": true
  },
  "gutter": {
    "line_numbers": false
  },
  "ui_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "ui_font_size": 15,
  "buffer_font_size": 15,
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "format_on_save": "on",
  "relative_line_numbers": true,
  "auto_install_extensions": {
    "dart": true,
    "lua": true,
    "ruby": true,
    "toml": true,
    "java": true,
    "kotlin": true,
    "liquid": true,
    "tmux": true,
    "sql": true,
    "git-firefly": true,
    "dockerfile": true,
    "docker-compose": true,
    "tokyo-night": true,
    "intellij-newui-theme": true,
    "wakatime": true
  }
}