{
  "agent": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4"
    },
    "version": "2"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": "JetBrains New Dark",
  "icon_theme": "JetBrains New UI Icons (Light)",
  "inlay_hints": {
    "enabled": true
  },
  "gutter": {
    "line_numbers": false
  },
  "ui_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "ui_font_size": 15,
  "buffer_font_size": 15,
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "format_on_save": "on",
  "relative_line_numbers": true,
  "auto_install_extensions": {
    "make": true,
    "terraform": true,
    "ansible": true,
    "dart": true,
    "lua": true,
    "ruby": true,
    "toml": true,
    "java": true,
    "scala": true,
    "kotlin": true,
    "liquid": true,
    "tmux": true,
    "sql": true,
    "git-firefly": true,
    "dockerfile": true,
    "docker-compose": true,
    "tokyo-night": true,
    "jetbrains-new-ui-icons": true,
    "intellij-newui-theme": true,
    "wakatime": true
  },
  "tab_bar": {
    "show": false,
    "show_nav_history_buttons": true,
    "show_tab_bar_buttons": true,
    "show_close_button": true
  }
}
