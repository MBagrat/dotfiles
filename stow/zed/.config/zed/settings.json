{
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "features": {
    "edit_prediction_provider": "zed",
    "inline_completions": true,
    "inline_completions_provider": "zed.dev"
  },
  "assistant": {
    "version": "2",
    "provider": "zed.dev",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    },
    "default_claude": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    },
    "inline_alternatives": [
      { "model": "claude-3-7-sonnet-latest", "provider": "zed.dev" }
    ]
  },
  "theme": {
    "mode": "system",
    "dark": "Tokyo Night",
    "light": "Tokyo Night Light"
  },
  "inlay_hints": {
    "enabled": true
  },
  "tab_bar": {
    "show": true
  },
  "ui_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "ui_font_size": 15,
  "buffer_font_size": 15,
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "format_on_save": "on",
  "relative_line_numbers": true,
  "auto_install_extensions": {
    "dart": true,
    "lua": true,
    "ruby": true,
    "toml": true,
    "java": true,
    "kotlin": true,
    "liquid": true,
    "tmux": true,
    "sql": true,
    "git-firefly": true,
    "dockerfile": true,
    "docker-compose": true,
    "tokyo-night": true,
    "intellij-newui-theme": true,
    "wakatime": true
  }
}